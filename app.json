{
  "expo": {
    "name": "popbox-studio",
    "slug": "popbox-studio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "products",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.louischan.popboxstudio",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ""
      },
      "package": "com.louischan.popboxstudio"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": null,
          "backgroundColor": "#000000"
        }
      ],
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1096354518994-1drt1oq3davk9bgab61v89t917a8kbpo"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "modular_headers": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ccd5e67b-a725-45e9-82ba-b918b0504986",
      }
    },
    "owner": "louisjames"
  }
}
